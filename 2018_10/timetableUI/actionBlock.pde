class actionBlock{
  
  String name;
  int startHour, startMin, endHour, endMin;
  
  actionBlock(int startHour, int startMin, int endHour, int endMin, String name){
    this.startHour = startHour;
    this.startMin = startMin;
    this.endHour = endHour;
    this.endMin = endMin;
    this.name = name;
  }
  
  
  
}